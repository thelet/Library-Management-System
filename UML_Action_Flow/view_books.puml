@startuml
start

:User clicks "View Books" in LibraryGUI;
:LibraryGUI requests all books from Library;

partition FrontEnd {
    :LibraryGUI sends request to Library;
}

partition BackEnd {
    :Library retrieves all books;
    :LibraryGUI receives book list;
    :LibraryGUI displays book list;
    :Logger logs "Viewed all books";
}

stop
@enduml
